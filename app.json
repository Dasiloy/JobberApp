{
  "expo": {
    "name": "Jobber",
    "slug": "Jobber",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "jobber",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "cover",
      "image": "./assets/images/splash.png",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "icon": {
        "light": "./assets/images/splash.png"
      },
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dasycreation.JobberApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "26e02a98-afbf-4efa-a083-269032e3cfc8"
      }
    }
  }
}
